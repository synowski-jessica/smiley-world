{
    "manifest_version": 2,
    "name": "Emoji Replacer",
    "version": "1.0",

    "icons": {
        "48": "img/iconeExtension.png"
      },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "css":["style.css"]
        }
    ],

    "browser_action": {
        "default_popup": "popup.html"
    }
}
